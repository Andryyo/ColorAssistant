{
  "include": [
    ".eslintrc.js",
    "./src/**/*"
  ],
  "exclude": [
    "build",
    "node_modules"
  ],
  "compilerOptions": {
    "target": "es2020",
    "module": "es2020",
    "moduleResolution": "node",
    "outDir": "./build",
    "baseUrl": "./src",
    "esModuleInterop": true,
    "jsx": "react",
    "allowJs": true
  },
}
  